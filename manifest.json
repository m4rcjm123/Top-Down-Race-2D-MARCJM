{
  "manifest_version": 3,
  "name": "Top Down Race 2D MARCJM",
  "version": "1.0",
  "description": "Unity game inside Chrome extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "permissions": [],
  "icons": {
    "16": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "Build/*",
        "TemplateData/*",
        "unity-loader.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
